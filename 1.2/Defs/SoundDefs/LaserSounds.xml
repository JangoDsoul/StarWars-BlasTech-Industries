<Defs>

	<!-- DC-17 Blaster Pistol Sound -->
 <SoundDef>
    <defName>DC-17_Blaster_Pistol_Shot</defName>
    <context>MapOnly</context>
    <eventNames />
           <maxVoices>4</maxVoices>
           <maxSimultaneous>1</maxSimultaneous>
    <subSounds>
      <li>
        <grains>
          <li Class="AudioGrain_Folder">
            <clipPath>BlasterSound/DC-17_Blaster_Pistol_Shot</clipPath>
          </li>
        </grains>
        <volumeRange>
          <min>80</min>
          <max>85</max>
        </volumeRange>
        <pitchRange>
          <min>0.935690</min>
          <max>1.055345</max>
        </pitchRange>
      </li>
    </subSounds>
 </SoundDef>
  
